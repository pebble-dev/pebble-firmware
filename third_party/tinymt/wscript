def configure(conf):
    pass

def build(bld):
    tinymt_sources = [
        'TinyMT/tinymt/tinymt32.c',
    ]

    tinymt_includes = [
        'TinyMT/tinymt',
    ]

    bld.stlib(source=tinymt_sources,
              includes=tinymt_includes,
              export_includes=tinymt_includes,
              target='tinymt32',
              use='pblibc')
